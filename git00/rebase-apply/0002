-    t.test('encodeValuesOnly', function (st) {
-        st.equal(
-            qs.stringify(
-                { a: 'b', c: ['d', 'e=f'], f: [['g'], ['h']] },
-                { encodeValuesOnly: true }
-            ),
-            'a=b&c[0]=d&c[1]=e%3Df&f[0][0]=g&f[1][0]=h'
-        );
-        st.equal(
-            qs.stringify(
-                { a: 'b', c: ['d', 'e'], f: [['g'], ['h']] }
-            ),
-            'a=b&c%5B0%5D=d&c%5B1%5D=e&f%5B0%5D%5B0%5D=g&f%5B1%5D%5B0%5D=h'
-        );
-        st.end();
